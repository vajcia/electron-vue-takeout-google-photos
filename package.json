{
    "name": "electron-vue-takeout-google-photos",
    "version": "1.0.0",
    "main": "electron/main.js",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "vite",
        "start": "electron .",
        "build-renderer": "vite build",
        "build": "yarn build-renderer && electron-builder"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^4.0.0",
        "electron": "^26.0.0",
        "electron-builder": "^24.0.0",
        "vite": "^5.0.0",
        "vue": "^3.3.0"
    },
    "dependencies": {
        "exiftool-vendored": "^30.2.0",
        "fs-extra": "^11.1.1"
    },
    "build": {
        "appId": "com.example.electron-vue-takeout-google-photos",
        "productName": "TakeoutExifFixer",
        "directories": {
            "buildResources": "assets",
            "output": "dist-build"
        },
        "files": [
            "dist/**/*",
            "electron/**/*"
        ],
        "extraMetadata": {
            "main": "electron/main.js"
        },
        "asar": true,
        "asarUnpack": [
            "**\\node_modules\\@exiftool-vendored\\exiftool\\**"
        ]
    }
}
